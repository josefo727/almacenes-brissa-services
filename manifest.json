{
  "name": "almacenes-brissa-services",
  "vendor": "almacenesbrissa",
  "version": "0.0.5",
  "title": "Almacenes Brissa Services",
  "description": "Backend and masterdara services",
  "mustUpdateAt": "2025-09-09",
  "categories": [],
  "dependencies": {},
  "builders": {
    "node": "7.x",
    "docs": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "policies": [
    {
      "name": "OMSViewer"
    },
    {
      "name": "ListOrders"
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "kioskoseventos.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "kioskotiendas.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    }
  ],
  "settingsSchema": {
    "title": "Almacenes Brissa Services Settings",
    "type": "object",
    "properties": {
      "appKey": {
        "title": "VTEX App Key",
        "type": "string"
      },
      "appToken": {
        "title": "VTEX App Token",
        "type": "string"
      },
      "subAccounts": {
        "title": "Sub-accounts",
        "description": "Comma-separated list of sub-accounts to sync to (e.g., kioskoseventos,kioskotiendas)",
        "type": "string"
      },
      "syncFields": {
        "title": "Fields to Sync",
        "description": "Comma-separated list of fields from the CL entity to sync",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
