{
  "memory": 256,
  "ttl": 10,
  "timeout": 30,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "events": {
    "sync-cl-manual-next": {
      "sender": "almacenesbrissa.almacenes-brissa-services",
      "keys": ["sync-cl-manual-next"]
    }
  },
  "routes": {
    "sync-cl-created": {
      "path": "/_v/sync/cl/created",
      "public": true
    },
    "sync-cl-updated": {
      "path": "/_v/sync/cl/updated",
      "public": true
    },
    "sync-cl": {
      "path": "/_v/sync/cl/manual",
      "public": true,
      "method": "POST"
    }
  }
}
